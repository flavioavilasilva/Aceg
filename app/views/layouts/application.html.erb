													<li><a href="/oferta_voluntariado" title="Adotar" class="gn-icon gn-icon-photoshop">Seja 													voluntário</a></li>
													<li><a href="/oferta_voluntariado/new" title="Adotar" class="gn-icon gn-icon-photoshop">Ofertas 													voluntáriado</a>
													</li>
												</ul>
												</li>
								        <li><a class="gn-icon gn-icon-cog">Quem somos</a></li>
												<li><a class="gn-icon gn-icon-cog">Contato</a></li>
								    </ul>
								</div><!-- /gn-scroller -->
			        </nav>
			    </li>
			    <li>
						<a href="http://localhost:3000/">Aceg</a></li>
			    <li>
					</li>
			    <!-- ... -->
			</ul>	
		</div>
	
		<div style="z-index:1001; position:absolute;right:1%;top:1%"><%= render "layouts/navigation" %></div>
	</div>
</div>

<div>
<%= yield %>
</div>
	
<footer>
</footer>
	
